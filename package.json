{
  "name": "myapp",
  "version": "1.0.0-beta.1",
  "author": "Vijay",
  "main": "electron.prod.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --base-href=\"./\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version-update": "node ./update-build-version.js",
    "electron-dev": "electron ./electron.dev.js",
    "electron-run-dev": "concurrently \"ng serve\" && \"npm run electron-dev\"",
    "electron-run-prod": "ng build --prod  && electron ./electron.prod.js",
    "package:win": "npm run build && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite ",
    "package:linux": "npm run build && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "package:osx": "npm run build && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run build && cross-var electron-packager dist/$npm_package_name $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/fire": "^5.1.1",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/pwa": "^0.13.0",
    "@angular/router": "^7.2.3",
    "@angular/service-worker": "^7.2.3",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-charts": "^10.0.0",
    "core-js": "^2.5.4",
    "date-fns": "^1.30.1",
    "guid-typescript": "^1.0.9",
    "hammerjs": "^2.0.8",
    "path": "^0.12.7",
    "pouchdb": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/pouchdb": "^6.3.2",
    "angular-in-memory-web-api": "^0.7.0",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "cross-var": "^1.1.0",
    "electron": "^4.1.2",
    "electron-packager": "^13.1.1",
    "emoji-flags": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "replace-in-file": "^3.4.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
